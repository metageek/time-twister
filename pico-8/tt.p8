pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
#include tt.lua
level = 3
score = 0
function _init()
  swarm = mkSwarm(level)
  player = mkPlayer()
end
function _update()
  for _, r in ipairs(recordedPlayers)
  do
    if r ~= nil
    then
      r.update()
    end
  end
  player.update()
  swarm.update()
end
function _draw()
  cls(7)
  for _, r in ipairs(recordedPlayers)
  do
    if r ~= nil
    then r.draw()
    end
  end
  swarm.draw()
  player.draw()

  local s = tostr(score)
  print(s, 64 - (#s*2), 0, 8)
end
