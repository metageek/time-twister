pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
#include tt.lua
function _init()
  swarm = mkSwarm(3)
  player = mkPlayer()
end
function _update()
  for _, r in ipairs(recordedPlayers)
  do
    if r ~= nil
    then
      r.update()
    end
  end
  player.update()
  swarm.update()
end
function _draw()
  cls(7)
  for _, r in ipairs(recordedPlayers)
  do
    if r ~= nil
    then r.draw()
    end
  end
  player.draw()
  swarm.draw()
end
